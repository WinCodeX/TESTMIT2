<div id="<%= dom_id student %>">
  <p>
    <strong>Name:</strong>
    <%= student.name %>
  </p>

  <p>
    <strong>Age:</strong>
    <%= student.age %>
  </p>

  <p>
    <strong>Level:</strong>
    <%= student.level %>
  </p>

  <p>
    <strong>From:</strong>
    <%= student.from %>
  </p>

  <p>
    <strong>Course:</strong>
    <%= student.course %>
  </p>

</div>
